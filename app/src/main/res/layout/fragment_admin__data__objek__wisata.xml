<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin_Data_Objek_Wisata"
    android:orientation="vertical">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:text="Kembali"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="50dp">
        <TextView
            android:id="@+id/text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Gambar"
            android:textSize="14pt"
            android:fontFamily="sans-serif-black"/>
        <Button
            android:id="@+id/button_unggah"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Unggah"
            android:layout_below="@id/text1"/>
        <ImageView
            android:id="@+id/image"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/text1"
            android:src="@drawable/ic_launcher_foreground"
            android:layout_marginLeft="30dp"
            android:background="#808080"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="30dp"
        android:background="#FFFFFF"
        android:layout_marginTop="10dp"
        android:padding="5dp">
        <TextView
            android:id="@+id/text2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Judul"
            android:textSize="9pt"
            android:fontFamily="sans-serif-black"
            android:paddingTop="10dp"
            />
        <TextView
            android:id="@+id/dot1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:textSize="8pt"
            android:fontFamily="sans-serif-black"
            android:layout_centerHorizontal="true"
            android:layout_toLeftOf="@id/judul"
            android:layout_marginRight="10dp"
            android:paddingTop="10dp"/>
        <EditText
            android:id="@+id/judul"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:text="x1234567890"
            android:textSize="9pt"/>
        <TextView
            android:id="@+id/text3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rating"
            android:textSize="9pt"
            android:fontFamily="sans-serif-black"
            android:layout_below="@id/text2"
            android:paddingTop="15dp"/>
        <TextView
            android:id="@+id/dot2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:textSize="8pt"
            android:fontFamily="sans-serif-black"
            android:layout_toLeftOf="@id/rating"
            android:layout_below="@+id/dot1"
            android:paddingTop="20dp"
            android:layout_marginRight="10dp"/>
        <EditText
            android:id="@+id/rating"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@id/judul"
            android:text="x1234567890"
            android:textSize="9pt"/>
        <TextView
            android:id="@+id/text4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Biaya Kunjungan"
            android:textSize="9pt"
            android:fontFamily="sans-serif-black"
            android:layout_below="@id/text3"
            android:paddingTop="15dp"/>
        <TextView
            android:id="@+id/dot3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:textSize="8pt"
            android:fontFamily="sans-serif-black"
            android:layout_toLeftOf="@id/biayakunjungan"
            android:layout_below="@+id/dot2"
            android:paddingTop="20dp"
            android:layout_marginRight="10dp"/>
        <EditText
            android:id="@+id/biayakunjungan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@id/rating"
            android:text="x1234567890"
            android:textSize="9pt"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="30dp"
        android:background="#FFFFFF"
        android:layout_marginTop="10dp"
        android:padding="5dp">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/text5"
            android:text="Biaya Perjalanan Ke"
            android:textSize="8pt"
            android:fontFamily="sans-serif-black"/>
        <TextView
            android:id="@+id/place1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tempat"
            android:textSize="8pt"
            android:fontFamily="sans-serif-black"
            android:layout_below="@id/text5"
            android:paddingTop="5dp"/>
        <TextView
            android:id="@+id/dot4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:textSize="9pt"
            android:fontFamily="sans-serif-black"
            android:layout_centerHorizontal="true"
            android:layout_toLeftOf="@id/biaya1"
            android:layout_marginRight="10dp"
            tools:ignore="DuplicateIds"
            android:layout_below="@id/text5"
            android:paddingTop="5dp"/>
        <EditText
            android:id="@+id/biaya1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:text="x1234567890"
            android:textSize="8pt"
            android:layout_below="@id/text5"/>
        <TextView
            android:id="@+id/place2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tempat2"
            android:textSize="8pt"
            android:fontFamily="sans-serif-black"
            android:layout_below="@id/place1"
            android:paddingTop="20dp"/>
        <TextView
            android:id="@+id/dot5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:textSize="9pt"
            android:fontFamily="sans-serif-black"
            android:layout_toLeftOf="@id/biaya2"
            android:layout_below="@+id/dot4"
            android:paddingTop="20dp"
            android:layout_marginRight="10dp"
            tools:ignore="DuplicateIds" />
        <EditText
            android:id="@+id/biaya2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@id/biaya1"
            android:text="x1234567890"
            android:textSize="8pt"/>
        <TextView
            android:id="@+id/place3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tempat3"
            android:textSize="8pt"
            android:fontFamily="sans-serif-black"
            android:layout_below="@id/place2"
            android:paddingTop="25dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:textSize="9pt"
            android:fontFamily="sans-serif-black"
            android:layout_toLeftOf="@id/biaya3"
            android:layout_below="@id/dot5"
            android:paddingTop="15dp"
            android:layout_marginRight="10dp"/>
        <EditText
            android:id="@+id/biaya3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@id/biaya2"
            android:text="x1234567890"
            android:textSize="8pt"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/rounded_background"
        android:layout_margin="20dp">
        <TextView
            android:id="@+id/text6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Deskripsi"
            android:layout_margin="10dp"/>
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:text="ubah"/>
        <TextView
            android:id="@+id/deskripsi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Deskripsi Objek Wisata"
            android:layout_below="@id/text6"
            android:layout_marginHorizontal="10dp"/>
    </RelativeLayout>
</LinearLayout>